{
  "name": "@lukasz-starosta/javascript-starter",
  "version": "0.1.6",
  "description": "JavaScript starter",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "start": "webpack-dev-server --env.entry=./src/index.js --config=./node_modules/@lukasz-starosta/micro-connect/webpack.config.js",
    "build": "webpack --env.production --env.entry=./src/index.js  --env.library=javascript-starter --config=./node_modules/@lukasz-starosta/micro-connect/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasz-starosta/micro-connect.git"
  },
  "dependencies": {
    "@lukasz-starosta/micro-connect": "^0.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^4.5.0"
  }
}
